[configuration]
entry_symbol = "terrain_generator_library_init"
compatible_godot_version = "4.2"
compatibility_minimum = "4.2"
reloadable=true

[files]
libterrain_generator = "res://addons/terrain_generator/bin/libterrain_generator.so"

[libraries]
linux.debug = "bin/libterrain_generator.so"
linux.release = "bin/libterrain_generator.so"
windows.debug = "bin/terrain_generator.dll"
windows.release = "bin/terrain_generator.dll"

